{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=employee-management-system;Trusted_Connection=True;"

  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Forwarded-For",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 1
      }
    ]
  },
  "IpRateLimitPolicies": {
    "Default": {
      "Rules": [
        {
          "Endpoint": "*",
          "Period": "1m",
          "Limit": 1
        }
      ]
    }
  },
  "DefaultAdmin": {
    "Email": "admin@system.com",
    "Password": "Admin@12345"
  },
  "Identity": {
    "MaxFailedAttempts": 5,
    "LockoutMinutes": 5,
    "SessionExpirationPeriod": 1,
    "TokenExpirationPeriod": 1,
    "TokenResetPeriod": 1
  },
  "Jwt": {
    "Key": "00 C1 A1 11 41 62 31 D5 B8 89 95 CD A3 D8 61 99 37 92 B8 20 69 01 F9 AE 63 12 C1 D9 19 09 5E 68 7E 0E 83 C4 1A 04 BC 36 33 DB FC BC CB F5 D9 33 FF 7F D8 36 B8 97 B9 45 C2 91 B8 4C AA 4D 38 80 47",
    "Issuer": "https://localhost:44300",
    "ExpireDays": 1
  },
  "Origins": "https://admin.claroeg.app;http://localhost:4500;http://localhost:4444;http://localhost:44220;http://localhost:4200",
  "BaseUrl": "https://localhost:4444",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "S3": {
    "Url": "https://test.claro.eg/",
    "BucketsName": "claro-test",
    "AccountId": "",
    "AccessKey": "",
    "SecretKey": ""
  },
  "Cequens": {
    "Uri": "",
    "SenderName": "",
    "Token": ""
  },
  "SendGrid": {
    "ApiKey": "",
    "FromEmail": "noreply@claro.eg",
    "FromName": ""
  },
  "Serilog": {
    "WriteTo:Seq": {
      "Name": "Seq",
      "Args": {
        "serverUrl": "http://localhost:5341"
      }
    },
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          {
            "Name": "Console",
            "Args": {
              "outputTemplate": "[{Timestamp:HH:mm:ss.fff zzz} {Level:u3}] ({RequestPath}) <src:{SourceContext}>{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}",
              "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
            }
          },
          {
            "Name": "Logger",
            "Args": {
              "configureLogger": {
                "Filter": [
                  {
                    "Name": "ByIncludingOnly",
                    "Args": {
                      "expression": "@l = 'Fatal'"
                    }
                  }
                ],
                "WriteTo": [
                  {
                    "Name": "File",
                    "Args": {
                      "path": "Logs/fetal_.log",
                      "shared": "true",
                      "rollingInterval": "Day",
                      "retainedFileCountLimit": 15,
                      "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
                    }
                  }
                ]
              }
            }
          },
          {
            "Name": "Logger",
            "Args": {
              "configureLogger": {
                "Filter": [
                  {
                    "Name": "ByIncludingOnly",
                    "Args": {
                      "expression": "@l = 'Error'"
                    }
                  }
                ],
                "WriteTo": [
                  {
                    "Name": "File",
                    "Args": {
                      "path": "Logs/err_.log",
                      "shared": "true",
                      "rollingInterval": "Day",
                      "retainedFileCountLimit": 15,
                      "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
                    }
                  }
                ]
              }
            }
          },
          {
            "Name": "Logger",
            "Args": {
              "configureLogger": {
                "Filter": [
                  {
                    "Name": "ByIncludingOnly",
                    "Args": {
                      "expression": "@l = 'Warning'"
                    }
                  }
                ],
                "WriteTo": [
                  {
                    "Name": "File",
                    "Args": {
                      "path": "Logs/warn_.log",
                      "shared": "true",
                      "rollingInterval": "Day",
                      "retainedFileCountLimit": 15,
                      "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
                    }
                  }
                ]
              }
            }
          }
        ]
      }
    },
    "Properties": {
      "Application": "ClaroEg"
    }
  }
}
